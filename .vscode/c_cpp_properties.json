{
    "env": {
        "AGRELEASE": "{workspaceFolder}"
    },
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${env:ROOTSYS}/include/**",
                "${env:ROOTANASYS}/include/**",
                "${env:ROOTANASYS}/manalyzer/**",
                "${workspaceFolder}/a2lib/include/**",
                "${workspaceFolder}/agana/**",
                "${workspaceFolder}/aged/include/**",
                "${workspaceFolder}/alpha2/include/**",
                "${workspaceFolder}/ana/include/**",
                "${workspaceFolder}/analib/include/**",
                "${workspaceFolder}/reco/include/**",
                "${workspaceFolder}/recolib/include/**",
                "${workspaceFolder}/rootUtils/include/**",
                "${workspaceFolder}/simulation/G4out/include"
            ],
            "defines": [
                "BUILD_AG_SIM"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "gcc-x64",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}